<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="description" content="" />
    <meta name="keywords" content="" />

    <link rel="canonical" href="https://www.galacticpuffin.com/" />
    <link rel="apple-touch-icon" href="~/image/a.png">
    <link rel="shortcut icon" href="~/images/siteicon.png" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>

    <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3 navbar-fixed">

        <div class="container">
            <div id="logo" class="logo">
                <a class="current" href="\"><span> </span></a>
            </div>
            @*<label for="navigation-1" class="hamburger"> <span>menu</span> </label>*@
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @*@Html.ActionLink(" ", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })*@
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">

                    <li>
                        <a href="@Url.Action("index", "home")" class="primayMenuText">

                            Home
                        </a>
                    </li>
                    <li>
                        <a href="@Url.Action("service", "home")" class="primayMenuText">

                            Service
                        </a>
                    </li>
                    <li>
                        <a href="@Url.Action("app", "home")" class="primayMenuText">

                            App Lab
                        </a>
                    </li>
                    <li>
                        <a href="@Url.Action("contact", "home")" class="primayMenuText">

                            Contact
                        </a>
                    </li>


                </ul>
            </div>
        </div>
    </nav>

    <div class="container body-content">
        @RenderBody()
        @*<hr />*@

    </div>

    <div id="wrap-footer" class="wrap-footer">
        <div id="footer" class="footer">
            <div class="copyright">

                <span>Copyright ©  Galactic Puffin All Rights Reserved.</span>
                <a href="@Url.Action("privacy", "home")" class="privacy">
                    Privacy Policy
                </a>

                @*<a class="divided" href="videopolicy.html">Video Policy</a>*@
                @*<a class="divided" href="https://www.facebook.com/Amanita.Design">Facebook</a>
                    <a class="divided" href="https://www.instagram.com/amanita_design_">Instagram</a>
                    <a class="divided" href="https://twitter.com/Amanita_Design">Twitter</a>
                    <a class="divided" href="https://discord.gg/Gr4Ec6K">Discord</a>
                    <a class="divided" href="https://www.youtube.com/user/jdamanita">YouTube</a>*@
            </div>

        </div>
        <!-- /footer -->
    </div>
    @*<p>&copy; @DateTime.Now.Year - Galactic Puffin</p>*@



    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
