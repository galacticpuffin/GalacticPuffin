ViewBag.Title = "Galactic Puffin";
}


<div class="row">
    <div class="w3-content" style="max-width:100%">
        <div class="one-time">
            @*<div> <img class="mySlides" src="~/images/banner-1.jpg" style="width:100%"></div>*@
            <div>
                <img class="mySlides" src="~/images/banner-2.jpg" style="width:100%">
            </div>
            @*<div> <img class="mySlides" src="~/images/b-3.png" style="width:100%;max-height:200px"></div>*@
